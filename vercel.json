{
    "version": 2,
	"buildCommand": "cd frontend && npm run build",
	"installCommand": "cd backend && npm i && cd ../frontend && npm i",
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}